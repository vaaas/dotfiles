TAB: menu-complete

set show-all-if-ambiguous on
set bind-tty-special-chars off

"\e\C-H": backward-word
"\e\C-L": forward-word
"\eH": beginning-of-line
"\eL": end-of-line
"\ed": kill-word
"\C-D": backward-kill-word
"\eh": backward-char
"\el": forward-char
"\ej": history-search-forward
"\ek": history-search-backward
"\e[P": delete-char
"\C-k": kill-whole-line
"\C-f": reverse-search-history
"\C-F": forward-search-history
