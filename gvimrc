"aesthetics
set guifont=Fira\ Code\ 16
set guioptions-=m  "remove menu bar
set guioptions-=T  "remove toolbar
set guioptions-=r  "remove right-hand scroll bar
set guioptions-=L  "remove left-hand scroll bar
colors low

noremap i h
noremap e j
noremap o k
noremap a l
noremap y i
noremap h a
noremap <Space> :

inoremap <C-Space> <C-n>

noremap <A-f> :e 
