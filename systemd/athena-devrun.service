[Unit]
Description=athena devrun
After=network-online.target
Wants=network-online.target

[Service]
Restart=on-abnormal
User=dev
Group=dev
WorkingDirectory=/home/dev/code/athena
ExecStart=/home/dev/code/athena/devrun.sh
KillMode=mixed
KillSignal=SIGTERM
TimeoutStopSec=5s

[Install]
WantedBy=multi-user.target
