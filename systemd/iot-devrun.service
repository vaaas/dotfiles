[Unit]
Description=iotwebserver devrun
After=network-online.target
Wants=network-online.target

[Service]
Restart=on-abnormal
User=vas
Group=vas
WorkingDirectory=/home/vas/Projects/iotwebserver
ExecStart=/home/vas/Projects/iotwebserver/devrun.sh
KillMode=mixed
KillSignal=SIGTERM
TimeoutStopSec=5s

[Install]
WantedBy=multi-user.target
