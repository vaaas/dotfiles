{
	"workbench.startupEditor": "newUntitledFile",
	"workbench.colorTheme": "White",
	"editor.minimap.enabled": false,
	"workbench.editor.enablePreview": false,
	"editor.fontFamily": "Fira Code, monospace",
	"scm.inputFontFamily": "editor",
	"debug.console.fontFamily": "editor",
	"window.menuBarVisibility": "toggle",
	"workbench.activityBar.visible": false,
	"breadcrumbs.enabled": false,
	"editor.renderWhitespace": "none",
	"editor.fontLigatures": true,
	"workbench.editor.showTabs": false,
	"workbench.statusBar.visible": false,
	"editor.autoIndent": "brackets",
	"editor.wordWrap": "on",
	"editor.tabCompletion": "on",
	"editor.lineNumbers": "off",
	"editor.cursorSmoothCaretAnimation": true,
	"editor.cursorStyle": "underline",
	
	"workbench.colorCustomizations": {
		"editor.background": "#fff3e0",
		"editorIndentGuide.background": "#ffcc80",
		"editorIndentGuide.activeBackground": "#ff9100",
		"editorWhitespace.foreground": "#ffcc80",
		"editor.lineHighlightBackground": "#ffffff",
		"sideBar.background": "#fff3e0",
		"sideBar.border": "#ffa726",
		"sideBarSectionHeader.background": "#ffe0b2",
		"list.inactiveSelectionBackground": "#ffe0b2",
		"list.activeSelectionForeground": "#ffe0b2",
		"list.hoverBackground": "#ffe0b2",
		"editorCursor.foreground": "#f50057",
		"editorGutter.modifiedBackground": "#0d47a1",
		"tab.activeForeground": "#000000"
	},
	"editor.tokenColorCustomizations": {
		"strings": "#00695c",
		"comments": "#e57373",
		"textMateRules": [
			{
				"scope": "keyword.control",
				"settings": { "fontStyle": "bold", "foreground": "#424242" }
			},
			{
				"scope": "keyword.other",
				"settings": { "fontStyle": "bold", "foreground": "#424242" }
			},
			{
				"scope": "constant",
				"settings": { "foreground": "#424242" }
			},
			{
				"scope": "constant.numeric",
				"settings": { "fontStyle": "", "foreground": "#0d47a1" }
			},

			{
				"scope": "support.class",
				"settings": { "foreground": "#424242" }
			},
			{
				"scope": "variable.language.this",
				"settings": { "foreground": "#752929", "fontStyle": "italic" }
			},
			{
				"scope": "storage.type.function.arrow",
				"settings": { "fontStyle": "" }
			},
			{
				"scope": "meta.object-literal.key",
				"settings": { "fontStyle": "underline"}
			},
			{
				"scope": "meta.definition",
				"settings": { "fontStyle": "underline" }
			},
			{
				"scope": "keyword.operator.comparison",
				"settings": { "fontStyle": "" }
			},
			{
				"scope": "keyword.operator.assignment",
				"settings": { "fontStyle": "" }
			},
			{
				"scope": "keyword.operator.new",
				"settings": { "fontStyle": "", "foreground": "#424242" }
			},
			{
				"scope": "meta.template.expression",
				"settings": { "foreground": "#000000", "fontStyle": "" }
			},
			{
				"scope": "storage.type",
				"settings": { "fontStyle": "underline" }
			},
			{
				"scope": "variable.parameter",
				"settings": { "fontStyle": "italic" }
			},
			{
				"scope": "variable.other",
				"settings": { "fontStyle": "", "foreground": "#000000"}
			}
		]
	},
	"editor.cursorBlinking": "solid",
	"editor.smoothScrolling": true,
	"editor.fontSize": 16,
	"search.exclude": {
		"**/public": true
	},
	"window.zoomLevel": 0,
}
