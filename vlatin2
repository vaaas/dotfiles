default partial alphanumeric_keys modifier_keys
xkb_symbols "basic" {
	name[Group1]= "English (Vas)";

	key <TLDE> {[ grave, asciitilde ]};

	key <AE01> {[ 1, exclam ]};
	key <AE02> {[ 2, at ]};
	key <AE03> {[ 3, numbersign]};
	key <AE04> {[ 4, dollar, sterling ]};
	key <AE05> {[ 5, percent ]};
	key <AE06> {[ 6, asciicircum ]};
	key <AE07> {[ 7, ampersand ]};
	key <AE08> {[ 8, asterisk ]};
	key <AE09> {[ 9, parenleft ]};
	key <AE10> {[ 0, parenright ]};
	key <AE11> {[ minus, underscore, emdash, endash ]};
	key <AE12> {[ equal, plus ]};

	key <AD01> {[ q, Q ]};
	key <AD02> {[ c, C ]};
	key <AD03> {[ d, D ]};
	key <AD04> {[ m, M ]};
	key <AD05> {[ v, V ]};
	key <AD06> {[ j, J ]};
	key <AD07> {[ w, W ]};
	key <AD08> {[ u, U ]};
	key <AD09> {[ k, K ]};
	key <AD10> {[ semicolon, colon, ellipsis ]};

	// all the bracket pairs: paren, bracket, brace, comparisons
	key <AD11> {[ parenleft, bracketleft, braceleft, less ]};
	key <AD12> {[ parenright, bracketright, braceright, greater ]};

	key <AC01> {[ n, N ]};
	key <AC02> {[ s, S ]};
	key <AC03> {[ t, T ]};
	key <AC04> {[ r, R ]};
	key <AC05> {[ h, H ]};
	key <AC06> {[ y, Y, yen ]};
	key <AC07> {[ i, I ]};
	key <AC08> {[ e, E, euro ]};
	key <AC09> {[ o, O ]};
	key <AC10> {[ a, A ]};
	key <AC11> {[ apostrophe, quotedbl, guillemotright ]};

	key <AB01> {[ z, Z ]};
	key <AB02> {[ f, F ]};
	key <AB03> {[ g, G ]};
	key <AB04> {[ l, L ]};
	key <AB05> {[ x, X ]};
	key <AB06> {[ b, B ]};
	key <AB07> {[ p, P ]};
	key <AB08> {[ comma, less, guillemotleft ]};
	key <AB09> {[ period, greater, guillemotright ]};
	key <AB10> {[ slash, question ]};

	key <BKSL> {[ backslash, bar ]};

	// caps lock is compose
	key <CAPS> {[ Multi_key ]};

	// right control is level 5
	key <RCTL> {[ ISO_Level5_Shift, ISO_Level5_Shift ]};

	// right alt is level 3
	key <RALT> {[ ISO_Level3_Shift, ISO_Level3_Shift ]};

	key <SPCE> {[ space ]};

	// two shifts = caps lock
	key <LFSH> {[ Shift_L, Caps_Lock ]};
	key <RTSH> {[ Shift_R, Caps_Lock ]};
};
