// Place your key bindings in this file to override the defaults
[
	{ "key": "alt+b", "command": "cursorWordLeft", "when": "textInputFocus" },
	{ "key": "alt+f", "command": "cursorWordEndRight", "when": "textInputFocus" },
	{ "key": "alt+n", "command": "cursorDown", "when": "textInputFocus" },
	{ "key": "alt+p", "command": "cursorUp", "when": "textInputFocus" },
	{ "key": "ctrl+e", "command": "deleteWordEndRight", "when": "textInputFocus" },
	{ "key": "ctrl+t", "command": "workbench.action.focusNextGroup" },
	{ "key": "ctrl+w", "command": "deleteWordLeft", "when": "textInputFocus" },
	{ "key": "f1", "command": "workbench.action.tasks.runTask" },
]
